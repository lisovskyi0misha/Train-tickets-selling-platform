<table class="object-list-table">
    <tr>
        <th class="objext-list-cell">
            <% if objects.first.class.method_defined?(:title) %>   
                <strong><%= t('.title' )%></strong>
            <% else %>
                <strong><%= t('.number') %></strong>
            <%end%>
        </th>
    </tr>

    <% objects.each do |object| %>
        <tr>
            <td class="objext-list-cell">
                <% if object.class.method_defined?(:title) %>
                    <%= object.title %>
                <% else %>
                    <%= object.number %><span><i class="bi bi-pencil-square"></i></span>
                <%end%>
                <%= link_to '', class: "btn", data: { station_id: object.id} do %>
                    <i class="bi bi-pencil-square"></i></span>
                <%end%>
                <%= bootstrap_form_for object, url: inline_update_admin_station_path(id: object.id), html: {style: "display: none"}  do |f| %>
                    <%= f.text_field :title%>
                    <%= f.submit %>
                <% end %>
            </td>
            <td class="objext-list-cell">
                <%= link_to t('common.show'), "#{objects.model.to_s.downcase}s/#{object.id}", class: 'btn btn-outline-success' %>
            </td>
            <td class="objext-list-cell">
                <%= link_to t('common.edit'), "#{objects.model.to_s.downcase}s/#{object.id}/edit", class: 'btn btn-outline-warning' %>
            </td>
            <td class="objext-list-cell">
                <%= button_to t('common.destroy'), "#{objects.model.to_s.downcase}s/#{object.id}", method: :delete, class: 'btn btn-outline-danger' %>
            </td>
        </tr>
    <% end %>
</table>
